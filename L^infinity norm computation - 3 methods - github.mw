<Group labelreference="L4347" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">SleeveNormComputation:=proc(sys)
local n0, n1, GCD, H, DH,tx,tomega,tomegaList,ListOfMax,nonzero,M,
rootiso, D1,d,fp,opp, i,delta,slopee,ShearedSys,TResultant,T,MaxRootIso,j,A,WList,IsoRootIso,rootiso3,L, L2, max_asym;

H:= sys[1];
DH:= sys[2];
if nops(Isolate(lcoeff(H,Gamma), maxroots=1))&lt;&gt;0 then return infinity; end if;
tomega:=resultant(H,DH,omega):
L2:= sort(Isolate(lcoeff(H,omega),output=interval),key = (proc (x) options operator, arrow; rhs(x)[1] end proc)); 
max_asym:=rhs(L2[nops(L2)]);
tx:= resultant(H,DH,Gamma):
L:=CoefficientList(tx,omega ):
M:=(sort([seq(abs(L[i]),i=1..numelems(L)-1)])[numelems(L)-1])/abs(L[numelems(L)]) +1:

rootiso:= sort(Isolate(tomega, Gamma, output=interval),key = (proc (x) options operator, arrow; rhs(x)[1] end proc));

if nops(rootiso)=0 then return Null; elif nops(rootiso)=1 then
D1:= rand()/rand(); else 
D1:=(sort([seq(  rhs(rootiso[i+1])[1]- rhs(rootiso[i])[2],i=1..numelems(rootiso)-1)])[1])/2: end if;
d:=degree(H):
opp:=[op(expand(H))]:
fp:=0:
slopee:=0:
for i from 1 to numelems(opp) do
if degree(opp[i])=d then fp:=fp+opp[i]  end if  end do:
delta:=D1: 
for i from 1 to (d+1) do
if subs([Gamma=((3*d+i)*delta)/((4*d+2)*M),omega=1],fp) &lt;&gt; 0  then slopee:= ((3*d+i)*delta)/((4*d+2)*M)   end if  end do:

ShearedSys:=subs([Gamma=T-slopee*omega],[H,DH]):
TResultant:=resultant(op(ShearedSys),omega): 
rootiso3:=sort(Isolate(TResultant, T , output=interval),key = (proc (x) options operator, arrow; rhs(x)[1] end proc)):
if nops(rootiso3)=0 then return Null; end if;
MaxRootIso:=rhs((rootiso3)[numelems(rootiso3)]):
A:=numelems(rootiso):
for j from 1 to A do
if max_asym = rhs(<Font foreground="[128,0,0]">rootiso[1-j+A])</Font> then return convert(max_asym, float); end if;
if MaxRootIso[1] &gt;= (rhs(rootiso[1-j+A])[1]-D1) and MaxRootIso[2] &lt;=(rhs(rootiso[1-j+A])[2]+D1) then 
return convert(max(rhs(rootiso[1-j+A]),max_asym),float): 
end if end do:
end proc:
</Text-field>
</Input>
</Group>
