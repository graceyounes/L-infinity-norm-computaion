<Group labelreference="L4343" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">C:=proc(list1)
local k, list ,i,indice,zeroelement,firstindex, nonzeroelement,nomofzeros,zeroindexlist,indexofzeros,s,q,qp,pq,listsofnonzeros,a,b,epsilonlist,epsilonlist1,epsilonis:

list:=list1;
for i from 1 to nops(list) while (list[1] = 0) do
<Font foreground="[128,0,0]">list:=[seq(list[i],i=2..nops(list))]; end do; </Font><Font foreground="[204,153,255]">#Removing the 0s at the beginning</Font><Font foreground="[128,0,0]">

</Font>for i from 1 to nops(list) while (list[nops(list)] = 0) do
<Font foreground="[128,0,0]">list:=[seq(list[i],i=1..nops(list)-1)]; end do; </Font><Font foreground="[204,153,255]">#Removing the 0s at the end</Font>


if member(0,list) then 

nonzeroelement:= [seq(`if`(    list[i] = 0 ,NULL,i), i=1..nops(list))];
zeroelement := [seq(`if`(    list[i] &lt;&gt;0 ,NULL,i), i=1..nops(list))];
firstindex := [seq(`if`(    zeroelement[i+1] - zeroelement[i]  = 1 ,NULL,i+1), i=1..nops(zeroelement)-1)];
indexofzeros:=zeroelement[[1,op(firstindex)]];

nomofzeros:= [seq(nonzeroelement[i+1]-nonzeroelement[i]-1, i= 1.. numelems(nonzeroelement)-1)];
s:= [seq(`if`( nomofzeros[i]=0 ,NULL,i), i=1..nops(nomofzeros))];
nomofzeros:= nomofzeros[s];

zeroindexlist:= [seq( [indexofzeros[i], nomofzeros[i] ],i=1..nops(nomofzeros) )];


q:= [seq( [seq( list[k], k=nomofzeros[i]+indexofzeros[i].. indexofzeros[i+1]-1 )] , i=1.. nops(indexofzeros)-1)];
qp:= [seq(list[i],i= nomofzeros[nops(indexofzeros)]+indexofzeros[nops(indexofzeros)].. nops(list) )];
pq:= [seq(list[i],i= 1.. indexofzeros[1]-1 )];
listsofnonzeros:= [pq,op(q),qp];

<Font foreground="[153,204,0]">#now i have the lists that i need zeroindexlist and listsofnonzeros</Font>

a:=0;
for i from 1 to nops(listsofnonzeros) do
a:=a+ (P(listsofnonzeros[i])-V(listsofnonzeros[i]));
end do:

epsilonlist:= [seq(`if`( type(zeroindexlist[i][2],odd) ,NULL,i), i=1..nops(zeroindexlist))];
epsilonlist1:=zeroindexlist[epsilonlist];

b:=0;
for i from 1 to nops(epsilonlist1) do
b:= b+ (-1)^((epsilonlist1[i][2])/2)*sign(list[epsilonlist1[i][1]-1]/list[epsilonlist1[i][1]+epsilonlist1[i][2]]);
end do:
return a+b ;

else return(P(list)-V(list));
end if:
end proc:</Text-field>
