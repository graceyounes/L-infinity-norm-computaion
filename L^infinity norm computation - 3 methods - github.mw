<Group labelreference="L4323" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">NormInfty:=proc(sys)
local n0, n1, GCD, H, DH, S, f, N, List,WList, Norminfty,sol,res, L2, max_asym;

H:= sys[1];
DH:= sys[2];
if nops(Isolate(lcoeff(H,Gamma), maxroots=1))&lt;&gt;0 then return infinity; end if;
L2:= sort(Isolate(lcoeff(H,omega),output=interval),key = (proc (x) options operator, arrow; rhs(x)[1] end proc)); 
max_asym:=rhs(L2[nops(L2)]);
sol:=sort(Isolate(sys,[Gamma,omega]),key = (proc (x) options operator, arrow; x[1] end proc)):
if nops(sol)=0 then return Null; else
return max(rhs(sol[numelems(sol)][1]), max_asym[1]);
end if
end proc:</Text-field>
</Input>
</Group>
