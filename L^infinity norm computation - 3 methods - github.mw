<Group labelreference="L4304" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">SystemOf:=proc(RationalMat)
local n0, n1, GCD, H, DH, S, f, N, List,WList, Norminfty,sol,res;
n0:=numer(simplify(Determinant(Gamma^2*IdentityMatrix(RowDimension(RationalMat))- Transpose(subs([s=-s],RationalMat)).RationalMat))):
n1:=subs(s=I*omega,n0):
GCD:=GreatestCommonDivisor(n1, diff(n1, omega)):
H:=n1/GCD:
H:= SquareFreePart(H):
DH:= diff(H, omega):
S:= [H,DH ]:
return S;
end proc:</Text-field>
</Input>
</Group>
