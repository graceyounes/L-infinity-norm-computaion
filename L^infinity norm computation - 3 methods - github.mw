<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">SR_NormInfty:=proc(sys)
local n0, n1, GCD, H, DH, S,res,src,rootiso,maxroot,nri,i,listofroots,j,a,signe,v,delt,V,P,list4,ii,ind,values_of_sr,<Font foreground="[128,0,0]">iso_with_constraints,signee,iso_with_constraints2,</Font>values_of_sr2, L2, max_asym;
H:= sys[1];
DH:= sys[2];
#return implicitplot(H, omega = -5..5, Gamma = -15..15,gridrefine=3);
if nops(Isolate(lcoeff(H,Gamma), maxroots=1))&lt;&gt;0 then return infinity; end if;
L2:= sort(Isolate(lcoeff(H,omega),output=interval),key = (proc (x) options operator, arrow; rhs(x)[1] end proc)); 
if nops(L2)=0 then max_asym:=0 : else
max_asym:=rhs(L2[nops(L2)]): end if:
src := SubresultantChain(H, DH, omega, R):
<Font foreground="[128,0,0]">res:=factor(simplify(</Font>SubresultantOfIndex(0, src, R)<Font foreground="[128,0,0]">/GreatestCommonDivisor(</Font>SubresultantOfIndex(0, src, R)<Font foreground="[128,0,0]">, diff(</Font>SubresultantOfIndex(0, src, R)<Font foreground="[128,0,0]">, Gamma))</Font>));
rootiso:=sort(Isolate(res,output=interval),key = (proc (x) options operator, arrow; rhs(x)[1] end proc));
rootiso:= [seq( rhs(rootiso[i]), i= 1.. nops(rootiso))];
#return convert(rootiso, float);
if nops(rootiso)=0 then return Null; end if;
nri:=numelems(rootiso);
for ind from 1 to nri do<Font underline="true" bold="false">
</Font><Font foreground="[128,0,0]">maxroot:= rootiso[1-ind+nri];</Font>
if max_asym = maxroot then return convert(max_asym, float); end if;
<Font foreground="[204,153,255]">#we construct the sequence of the leading coefficients in Sturm-Habicht sequence</Font><Font foreground="[51,102,255]">
</Font><Font foreground="[128,0,0]">a:=[]; 
for ii from 1 to degree(DH,omega)-1 do 
a:= [op(a),lcoeff(</Font>SubresultantOfIndex(ii, src, R),omega<Font foreground="[128,0,0]">)]: end do:
a:=[op(a),lcoeff(DH,omega),lcoeff(H,omega)]:</Font>

<Font foreground="[204,153,255]">#Sign list using the option exactinterval</Font><Font foreground="[0,0,128]">
</Font><Font foreground="[128,0,0]">signe:=[];
if maxroot[1]= maxroot[2] then
signe:= [seq( SSign3(res,a[i],maxroot),i=1..nops(a))];
else
iso_with_constraints2:= fgbrs:-rs_isolate_uni_classical(res,exactinterval=maxroot,constraints=a):
values_of_sr2:= iso_with_constraints2[2][1]:
for i from 1 to nops (values_of_sr2) do
if values_of_sr2[i][1]=0 then signe:=[op(signe), 0]: else signe:=[op(signe), sign(values_of_sr2[i][1])]: end if:end do:
end if;</Font><Font foreground="[0,0,128]">
</Font><Font foreground="[204,153,255]">#signe is a list of the value(if zero) and the sign (if not zero) of the leading coefficients sequence over the root of the resultant in question</Font><Font foreground="[128,0,0]">
v:= degree(H,omega)-1;
delt:=proc(k)
return ((-1)^(k*(k+1)/2));
end proc;
list4:=[];
for i from 1 to (degree(H,omega)) do
list4:=[op(list4), signe[i]*delt(v-i)];
end do;</Font>
if C(list4)&gt;0 then return(convert(maxroot,float));end if;
end do;
end proc:</Text-field>
